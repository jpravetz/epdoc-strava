{
  "launch": {
    "groups": {
      "test": {
        "name": "Tests",
        "console": "internalConsole",
        "includes": [
          "**/*.test.ts"
        ],
        "runtimeArgs": [
          "test",
          "-A",
          "--inspect-brk"
        ]
      },
      "run": {
        "name": "Runnable",
        "console": "internalConsole",
        "includes": [
          "**/*.run.ts"
        ],
        "runtimeArgs": [
          "run",
          "-A",
          "--inspect-brk"
        ]
      }
    },
    "$schema": "https://raw.githubusercontent.com/epdoc/tools/master/packages/launchgen/schemas/v2/launch.schema.json"
  }
}
