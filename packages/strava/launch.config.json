{
  "launch": {
    "groups": {
      "test": {
        "name": "Tests",
        "console": "internalConsole",
        "includes": [
          "**/*.test.ts"
        ],
        "runtimeArgs": [
          "test",
          "-A",
          "--inspect-brk"
        ]
      },
      ".": {
        "name": "main",
        "console": "internalConsole",
        "program": "./main.ts",
        "runtimeArgs": [
          "run",
          "-A",
          "--inspect-brk"
        ],
        "scripts": [
          "",
          "--help",
          "athlete"
        ]
      },
      "kml": {
        "name": "main kml",
        "console": "internalConsole",
        "program": "./main.ts",
        "runtimeArgs": [
          "run",
          "-A",
          "--inspect-brk"
        ],
        "scriptArgs": "kml -o /Users/jpravetz/dev/@epdoc/strava/tmp/Activities.kml",
        "scripts": [
          "--help",
          "-d 20251012",
          "-d 20251012 -m",
          "-d 20251012 -mel",
          "-d 20251012 -mel -t Ride",
          "-d 20251012 -mel -t Walk",
          "-d 20251012 -mel -t Hike"
        ]
      },
      "gpx": {
        "name": "main gpx",
        "console": "internalConsole",
        "program": "./main.ts",
        "runtimeArgs": [
          "run",
          "-A",
          "--inspect-brk"
        ],
        "scriptArgs": "gpx",
        "scripts": [
          "--help",
          "-d 20251012",
          "-d 20251012 -m",
          "-d 20251012 -mel",
          "-d 20251012 -mel -t Ride",
          "-d 20251012 -mel -t Walk",
          "-d 20251113- -l"
        ]
      },
      "pdf": {
        "name": "main pdf",
        "console": "internalConsole",
        "program": "./main.ts",
        "runtimeArgs": [
          "run",
          "-A",
          "--inspect-brk"
        ],
        "scriptArgs": "pdf -o /Users/jpravetz/dev/@epdoc/strava/tmp/Activities.xml",
        "scripts": [
          "--help",
          "-d 20251011"
        ]
      }
    },
    "$schema": "https://raw.githubusercontent.com/epdoc/tools/master/packages/launchgen/schemas/v2/launch.schema.json"
  }
}
