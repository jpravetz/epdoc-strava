        "xmlbuilder": "^13.0.2"
    },
    "devDependencies": {
        "@types/jest": "^24.0.19",
        "@types/koa": "^2.0.51",
        "@types/koa-router": "^7.0.42",
        "@types/node": "^12.11.1",
        "@types/superagent": "^4.1.3",
